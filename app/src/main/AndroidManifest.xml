<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nvqquy98.moneyqq">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.nvqquy98.moneyqq.mainapp.MainApp"
        android:allowBackup="false"
        android:icon="@mipmap/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.moneyqq"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.nvqquy98.moneyqq.mainapp.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.moneyqq">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>